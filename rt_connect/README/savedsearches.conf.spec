# RT alert settings

action.rt_connect = [0|1]
* Enable RT Connect

action.rt_connect.param.queue = <int>
* Id of the queue to create the ticket in
* (required)

action.rt_connect.param.subject = <string>
* The subject of the ticket in RT
* (required)

action.rt_connect.param.content = <string>
* The text content of the ticket in RT
* (required)

